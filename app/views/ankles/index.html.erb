<p id="notice"><%= notice %></p>

<h1>Listing Ankles</h1>

<table class="box-center">
  <thead>
    <tr>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ankles.each do |ankle| %>
      <tr>
        <td><%= ankle.image %></td>
        <td><%= link_to 'Show', ankle %></td>
        <td><%= link_to 'Edit', edit_ankle_path(ankle) %></td>
        <td><%= link_to 'Destroy', ankle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ankle', new_ankle_path %>
